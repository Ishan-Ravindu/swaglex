{
  "name": "swaglex",
  "version": "1.0.0",
  "description": "ðŸš€ Production-ready Swagger documentation framework with advanced authentication, multi-tenancy, and enterprise features",
  "main": "index.js",
  "bin": {
    "swagger-docs": "./bin/cli.js"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run lint && npm run test && npm run format:check",
    "prepare": "husky install",
    "preversion": "npm run validate",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "clean": "rm -rf node_modules coverage dist",
    "docs:build": "jsdoc -c jsdoc.conf.json",
    "docs:serve": "http-server docs -p 8080",
    "security:audit": "npm audit",
    "security:check": "npm audit --audit-level moderate"
  },
  "keywords": [
    "swagger",
    "openapi",
    "api-documentation",
    "api-docs",
    "swagger-ui",
    "authentication",
    "rbac",
    "role-based-access-control",
    "multi-tenant",
    "enterprise",
    "production-ready",
    "express",
    "nodejs",
    "rest-api",
    "documentation-generator",
    "oauth",
    "jwt",
    "security",
    "middleware",
    "cli"
  ],
  "author": {
    "name": "Swaglex Team",
    "email": "team@swaglex.dev",
    "url": "https://swaglex.dev"
  },
  "homepage": "https://swaglex.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/swaglex/swaglex.git"
  },
  "bugs": {
    "url": "https://github.com/swaglex/swaglex/issues"
  },
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "connect-sqlite3": "^0.9.13",
    "sequelize": "^6.35.0",
    "sqlite3": "^5.1.6",
    "bcryptjs": "^2.4.3",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0",
    "cors": "^2.8.5",
    "commander": "^11.1.0",
    "inquirer": "^8.2.6",
    "chalk": "^4.1.2",
    "fs-extra": "^11.2.0",
    "joi": "^17.11.0",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-github2": "^0.1.12",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "node-cache": "^5.1.2",
    "redis": "^4.6.10",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "express-slow-down": "^2.0.1",
    "csurf": "^1.11.0",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "dotenv": "^16.3.1"
  },
  "optionalDependencies": {
    "mysql2": "^3.5.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "mariadb": "^3.2.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "@types/node": "^20.10.0",
    "nyc": "^15.1.0",
    "codecov": "^3.8.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "lib/",
    "bin/",
    "index.js",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
}
